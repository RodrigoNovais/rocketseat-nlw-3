        "koa-bodyparser": "^4.3.0",
        "koa-helmet": "^6.0.0",
        "koa2-cors": "^2.0.6",
        "multer": "^1.4.2",
        "objection": "^2.2.3",
        "pg": "^8.4.1",
        "sqlite3": "^5.0.0"
    },
    "devDependencies": {
        "@types/koa": "^2.11.5",
        "@types/koa-bodyparser": "^4.3.0",
        "@types/koa-helmet": "^5.2.0",
        "@types/koa2-cors": "^2.0.1",
