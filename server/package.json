{
    "name": "server",
    "version": "1.0.0",
    "private": true,
    "main": "dist/index.js",
    "scripts": {
        "build": "tsc",
        "start": "tsnd --transpile-only --ignore-watch node_modules --respawn -r dotenv/config src/index.ts",
